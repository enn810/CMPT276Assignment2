<!DOCTYPE html>
<html>

<head>
  <title>Person Database</title>
</head>

<body>
  <div class="container">
    <h2>Database Results</h2>

    <div>
      <table>
        <tr>
          <th>UID</th>
          <th>Name</th>
          <th>Size</th>
          <th>Height</th>
          <th>Type</th>
          <th>Year of Birth</th>
          <th>Superpower</th>
          <th>Hair Colour</th>
        </tr>
        <% rows.forEach(function(r) { %>
        <tr>
          <th><%= r.id %></th>
          <th><%= r.name %></th>
          <th><%= r.size %></th>
          <th><%= r.height %></th>
          <th><%= r.type %></th>
          <th><%= r.year %></th>
          <th><%= r.superpower %></th>
          <th><%= r.hair %></th>
        </tr>
        <% }); %>
      </table>
    </div>
</body>


</html>